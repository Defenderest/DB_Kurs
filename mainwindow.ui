QMainWindow {
    background-color: #ffffff;
    color: #333333;
}

QWidget {
    font-family: &quot;Segoe UI&quot;, system-ui, -apple-system, BlinkMacSystemFont, sans-serif;
    font-size: 10pt;
}


QTabWidget::pane {
    border: 1px solid #e0e0e0;
    border-top: none;
    background-color: #ffffff;
    border-radius: 0 0 6px 6px;
    padding: 20px;
}

QTabBar::tab {
    padding: 12px 20px;
    margin-right: 1px;
    background-color: #f5f5f5;
    border: 1px solid #e0e0e0;
    border-bottom: none;
    color: #555555;
    font-weight: 500;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
}

QTabBar::tab:selected {
    background-color: #ffffff;
    color: #0078d4;
    font-weight: 600;
    border-color: #e0e0e0;

}

QTabBar::tab:hover:!selected {
    background-color: #eeeeee;
    color: #333333;
    border-color: #dcdcdc;
}

QTabWidget::tab-bar {
    alignment: left;
    left: 5px;
}


QLineEdit, QComboBox, QDateEdit {
    padding: 8px 12px;
    border: 1px solid #dcdcdc;
    border-radius: 5px;
    background-color: #ffffff;
    color: #333333;
    min-height: 36px;
    font-size: 10pt;
}

QLineEdit:focus, QComboBox:focus, QDateEdit:focus {
    border-color: #7bbfff;

}

QLineEdit::placeholder {
    color: #aaaaaa;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 25px;
    border-left: 1px solid #dcdcdc;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
}

QComboBox::down-arrow {

     width: 10px;
     height: 10px;
}
QComboBox QAbstractItemView {
    border: 1px solid #dcdcdc;
    background-color: #ffffff;
    selection-background-color: #e6f2ff;
    selection-color: #0078d4;
    padding: 4px;
}


QPushButton {
    padding: 8px 16px;
    background-color: #0078d4;
    color: white;
    border: 1px solid #0078d4;
    border-radius: 5px;
    font-weight: 500;
    min-height: 36px;
    font-size: 10pt;
}
QPushButton:hover {
    background-color: #106ebe;
    border-color: #106ebe;
}
QPushButton:pressed {
    background-color: #005a9e;
    border-color: #005a9e;
}
QPushButton:disabled {
    background-color: #f0f0f0;
    color: #aaaaaa;
    border-color: #e0e0e0;
}


QPushButton#cartButton, QPushButton#wishlistButton, QPushButton#profileButton {
    background-color: transparent;
    border: none;
    padding: 6px;
    border-radius: 50%;
    min-height: 38px; min-width: 38px;
    max-width: 38px; max-height: 38px;
    color: #555555;
    font-size: 14pt;
}
QPushButton#cartButton:hover, QPushButton#wishlistButton:hover, QPushButton#profileButton:hover {
    background-color: #f0f0f0;
    color: #333333;
}
QPushButton#cartButton:pressed, QPushButton#wishlistButton:pressed, QPushButton#profileButton:pressed {
    background-color: #e0e0e0;
}


QPushButton#filterButton, QPushButton#sortButton, QPushButton#viewModeButton {
    background-color: #ffffff;
    border: 1px solid #dcdcdc;
    color: #555555;
    min-width: 38px; max-width: 38px;
    min-height: 36px; max-height: 36px;
    padding: 6px;
    border-radius: 5px;
    font-size: 12pt;
}
QPushButton#filterButton:hover, QPushButton#sortButton:hover, QPushButton#viewModeButton:hover {
    background-color: #f5f5f5;
    border-color: #c8c8c8;
    color: #333333;
}
QPushButton#filterButton:pressed, QPushButton#sortButton:pressed, QPushButton#viewModeButton:pressed {
    background-color: #e8e8e8;
}
QPushButton#viewModeButton:checked {
    background-color: #e6f2ff;
    border-color: #7bbfff;
    color: #0078d4;
}



QScrollArea {
    border: none;
    background-color: transparent;
}
QScrollBar:vertical {
    border: none;
    background: #f5f5f5;
    width: 8px;
    margin: 0px 0px 0px 0px;
    border-radius: 4px;
}
QScrollBar::handle:vertical {
    background: #dcdcdc;
    min-height: 25px;
    border-radius: 4px;
}
QScrollBar::handle:vertical:hover {
    background: #c8c8c8;
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none; background: none; height: 0px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QScrollBar:horizontal {
    border: none; background: #f5f5f5; height: 8px; margin: 0px 0px 0px 0px; border-radius: 4px;
}
QScrollBar::handle:horizontal {
    background: #dcdcdc; min-width: 25px; border-radius: 4px;
}
QScrollBar::handle:horizontal:hover {
    background: #c8c8c8;
}
QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    border: none; background: none; width: 0px;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}


QStatusBar {
    background-color: #f5f5f5;
    color: #555555;
    font-size: 9pt;
    border-top: 1px solid #e0e0e0;
}


QLabel {
   color: #333333;
