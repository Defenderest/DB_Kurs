QPushButton#navHomeButton,
QPushButton#navBooksButton,
QPushButton#navAuthorsButton,
QPushButton#navOrdersButton,
QPushButton#navProfileButton {
    padding: 10px 10px; /* Збільшено горизонтальний padding */
    border: none;
    border-radius: 8px; /* Збільшено заокруглення */
