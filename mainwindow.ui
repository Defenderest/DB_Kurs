QWidget {
    font-family: "Segoe UI", system-ui, -apple-system, BlinkMacSystemFont, sans-serif;
    font-size: 10pt;
}

QTabWidget::pane {
    border: 1px solid #dee2e6;
    border-top: none;
    background-color: #ffffff;
    border-radius: 0 0 4px 4px;
    padding: 15px;
}

QTabBar::tab {
    padding: 10px 18px;
    margin-right: 2px;
    background-color: #e9ecef;
    border: 1px solid #dee2e6;
    border-bottom: none;
    color: #495057;
    font-weight: 500;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QTabBar::tab:selected {
    background-color: #ffffff;
    color: #0d6efd;
    font-weight: 600;
    border-color: #dee2e6;
    border-bottom: 1px solid #ffffff;
    margin-bottom: -1px;
}

QTabBar::tab:hover:!selected {
    background-color: #dee2e6;
    color: #212529;
    border-color: #ced4da;
}

QTabWidget::tab-bar {
    alignment: left;
    left: 5px;
}

QLineEdit, QComboBox, QDateEdit {
    padding: 8px 10px;
    border: 1px solid #ced4da;
    border-radius: 4px;
    background-color: #ffffff;
    color: #212529;
    min-height: 34px;
    font-size: 10pt;
}

QLineEdit:focus, QComboBox:focus, QDateEdit:focus {
    border-color: #86b7fe;
    box-shadow: 0 0 0 0.2rem rgba(13, 110, 253, 0.25);
}

QLineEdit::placeholder {
    color: #6c757d;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid #ced4da;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}

QComboBox::down-arrow {
     image: url(:/icons/down-arrow.svg);
     width: 10px;
     height: 10px;
}
QComboBox QAbstractItemView {
    border: 1px solid #ced4da;
    background-color: #ffffff;
    selection-background-color: #cfe2ff;
    selection-color: #000;
    padding: 4px;
}

QPushButton {
    padding: 8px 14px;
    background-color: #0d6efd;
    color: white;
    border: 1px solid #0d6efd;
    border-radius: 4px;
    font-weight: 500;
    min-height: 34px;
    font-size: 10pt;
    transition: background-color 0.15s ease-in-out, border-color 0.15s ease-in-out;
}
QPushButton:hover {
    background-color: #0b5ed7;
    border-color: #0a58ca;
}
QPushButton:pressed {
    background-color: #0a58ca;
    border-color: #0a53be;
}
QPushButton:disabled {
    background-color: #6c757d;
    color: #ffffff;
    border-color: #6c757d;
    opacity: 0.65;
}

QPushButton#cartButton, QPushButton#wishlistButton, QPushButton#profileButton {
    background-color: transparent;
    border: 1px solid transparent;
    padding: 6px;
    border-radius: 4px;
    min-height: 36px; min-width: 36px;
    max-width: 36px; max-height: 36px;
    color: #495057;
    font-size: 14pt;
}
QPushButton#cartButton:hover, QPushButton#wishlistButton:hover, QPushButton#profileButton:hover {
    background-color: #e9ecef;
    color: #212529;
    border-color: #dee2e6;
}
QPushButton#cartButton:pressed, QPushButton#wishlistButton:pressed, QPushButton#profileButton:pressed {
    background-color: #dee2e6;
    border-color: #ced4da;
}

QPushButton#filterButton, QPushButton#sortButton, QPushButton#viewModeButton {
    background-color: #ffffff;
    border: 1px solid #ced4da;
    color: #495057;
    min-width: 36px; max-width: 36px;
    min-height: 34px; max-height: 34px;
    padding: 6px;
    border-radius: 4px;
    font-size: 12pt;
}
QPushButton#filterButton:hover, QPushButton#sortButton:hover, QPushButton#viewModeButton:hover {
    background-color: #f8f9fa;
    border-color: #adb5bd;
    color: #212529;
}
QPushButton#filterButton:pressed, QPushButton#sortButton:pressed, QPushButton#viewModeButton:pressed {
    background-color: #e9ecef;
}
QPushButton#viewModeButton:checked {
    background-color: #cfe2ff;
    border-color: #86b7fe;
    color: #0d6efd;
}

QScrollArea {
    border: none;
    background-color: transparent;
}
QScrollBar:vertical {
    border: none;
