   <string>/* --- Загальні стилі --- */
QMainWindow {
    background-color: #f8f9fa; /* Світло-сірий фон */
    color: #212529; /* Темний текст */
}

QWidget {
    font-family: &quot;Segoe UI&quot;, system-ui, -apple-system, BlinkMacSystemFont, sans-serif;
    font-size: 10pt;
}

/* --- Вкладки --- */
QTabWidget::pane {
    border: 1px solid #dee2e6; /* Світліша рамка */
    border-top: none;
    background-color: #ffffff; /* Білий фон вмісту вкладки */
    border-radius: 0 0 4px 4px;
    padding: 15px;
}

QTabBar::tab {
    padding: 10px 18px;
    margin-right: 2px;
    background-color: #e9ecef; /* Трохи темніший сірий для неактивних */
    border: 1px solid #dee2e6;
    border-bottom: none;
    color: #495057; /* Сірий текст */
    font-weight: 500;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QTabBar::tab:selected {
    background-color: #ffffff; /* Білий фон для активної */
    color: #0d6efd; /* Синій акцент */
    font-weight: 600;
    border-color: #dee2e6;
    border-bottom: 1px solid #ffffff; /* Приховуємо нижню межу */
    margin-bottom: -1px; /* Щоб перекрити межу панелі */
}

QTabBar::tab:hover:!selected {
    background-color: #dee2e6; /* Світліший сірий при наведенні */
    color: #212529;
    border-color: #ced4da;
}

QTabWidget::tab-bar {
    alignment: left;
    left: 5px; /* Невеликий відступ */
}

/* --- Поля вводу, Комбобокси, Дата --- */
QLineEdit, QComboBox, QDateEdit {
    padding: 8px 10px;
    border: 1px solid #ced4da; /* Стандартна сіра рамка */
    border-radius: 4px; /* Менш заокруглені кути */
    background-color: #ffffff;
    color: #212529;
    min-height: 34px; /* Трохи менша висота */
    font-size: 10pt;
}

QLineEdit:focus, QComboBox:focus, QDateEdit:focus {
    border-color: #86b7fe; /* Світло-синя рамка при фокусі */
    box-shadow: 0 0 0 0.2rem rgba(13, 110, 253, 0.25); /* Тінь як у Bootstrap */
}

QLineEdit::placeholder {
    color: #6c757d; /* Сірий плейсхолдер */
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid #ced4da;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}

QComboBox::down-arrow {
    /* Можна використати іконку або залишити стандартну */
     image: url(:/icons/down-arrow.svg); /* Приклад шляху до іконки */
     width: 10px;
     height: 10px;
}
QComboBox QAbstractItemView {
    border: 1px solid #ced4da;
    background-color: #ffffff;
    selection-background-color: #cfe2ff; /* Світло-синій фон виділення */
    selection-color: #000; /* Чорний текст виділення */
    padding: 4px;
}

/* --- Кнопки --- */
QPushButton {
    padding: 8px 14px;
    background-color: #0d6efd; /* Основний синій */
    color: white;
    border: 1px solid #0d6efd;
    border-radius: 4px;
    font-weight: 500;
    min-height: 34px;
    font-size: 10pt;
    transition: background-color 0.15s ease-in-out, border-color 0.15s ease-in-out; /* Плавний перехід */
}
QPushButton:hover {
    background-color: #0b5ed7; /* Темніший синій при наведенні */
    border-color: #0a58ca;
}
QPushButton:pressed {
    background-color: #0a58ca; /* Ще темніший при натисканні */
    border-color: #0a53be;
}
QPushButton:disabled {
    background-color: #6c757d; /* Сірий для неактивних */
    color: #ffffff;
    border-color: #6c757d;
    opacity: 0.65;
}

/* --- Кнопки-іконки в хедері --- */
QPushButton#cartButton, QPushButton#wishlistButton, QPushButton#profileButton {
    background-color: transparent;
    border: 1px solid transparent; /* Прозора рамка для збереження розміру */
    padding: 6px;
    border-radius: 4px; /* Звичайний радіус */
    min-height: 36px; min-width: 36px;
    max-width: 36px; max-height: 36px;
    color: #495057; /* Сірий колір іконки */
    font-size: 14pt; /* Розмір можна налаштувати */
}
QPushButton#cartButton:hover, QPushButton#wishlistButton:hover, QPushButton#profileButton:hover {
    background-color: #e9ecef; /* Світло-сірий фон при наведенні */
    color: #212529;
    border-color: #dee2e6;
}
QPushButton#cartButton:pressed, QPushButton#wishlistButton:pressed, QPushButton#profileButton:pressed {
    background-color: #dee2e6; /* Темніший сірий при натисканні */
    border-color: #ced4da;
}

/* --- Кнопки фільтрів/сортування/вигляду --- */
QPushButton#filterButton, QPushButton#sortButton, QPushButton#viewModeButton {
    background-color: #ffffff;
    border: 1px solid #ced4da; /* Стандартна сіра рамка */
    color: #495057; /* Сірий колір іконки/тексту */
    min-width: 36px; max-width: 36px; /* Зробимо квадратними */
    min-height: 34px; max-height: 34px;
    padding: 6px;
    border-radius: 4px;
    font-size: 12pt;
}
QPushButton#filterButton:hover, QPushButton#sortButton:hover, QPushButton#viewModeButton:hover {
    background-color: #f8f9fa; /* Дуже світлий сірий фон */
    border-color: #adb5bd;
    color: #212529;
}
QPushButton#filterButton:pressed, QPushButton#sortButton:pressed, QPushButton#viewModeButton:pressed {
    background-color: #e9ecef; /* Трохи темніший сірий */
}
QPushButton#viewModeButton:checked {
    background-color: #cfe2ff; /* Світло-синій фон для вибраного */
    border-color: #86b7fe;
    color: #0d6efd;
}

/* --- Скролбари --- */
QScrollArea {
    border: none;
    background-color: transparent;
}
QScrollBar:vertical {
    border: none;
    background: #f1f3f5; /* Дуже світлий фон */
    width: 6px; /* Тонший скролбар */
    margin: 0px 0px 0px 0px;
    border-radius: 3px;
}
QScrollBar::handle:vertical {
    background: #adb5bd; /* Сірий повзунок */
    min-height: 20px;
    border-radius: 3px;
}
QScrollBar::handle:vertical:hover {
    background: #6c757d; /* Темніший при наведенні */
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none; background: none; height: 0px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QScrollBar:horizontal {
    border: none; background: #f1f3f5; height: 6px; margin: 0px 0px 0px 0px; border-radius: 3px;
}
QScrollBar::handle:horizontal {
    background: #adb5bd; min-width: 20px; border-radius: 3px;
}
QScrollBar::handle:horizontal:hover {
    background: #6c757d;
}
QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    border: none; background: none; width: 0px;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}

/* --- Статус бар --- */
QStatusBar {
    background-color: #e9ecef; /* Світло-сірий фон */
    color: #495057; /* Сірий текст */
    font-size: 9pt;
    border-top: 1px solid #dee2e6; /* Тонка верхня межа */
}

/* --- Мітки (Labels) --- */
QLabel {
   color: #212529; /* Основний темний текст */
   background-color: transparent;
}
QLabel#logoLabel {
   font-size: 16pt; /* Трохи менший логотип */
   font-weight: 600;
   color: #000000; /* Чорний */
   padding-left: 0px;
}
QLabel#bannerLabel {
   /* background-color: qlineargradient(spread:pad, x1:0, y1:0.5, x2:1, y2:0.5, stop:0 #e6f2ff, stop:1 #d0e7ff); */
   background-color: #cfe2ff; /* Простий світло-синій фон */
   border-radius: 6px; /* Менше заокруглення */
   color: #0a58ca; /* Темніший синій текст */
   font-size: 20pt; /* Менший розмір шрифту */
   font-weight: 500; /* Звичайний шрифт */
   padding: 25px;
   text-align: center; /* Вирівнювання по центру */
}
/* Заголовки секцій */
QLabel#categoryHeaderLabel, QLabel#bestsellerHeaderLabel, QLabel#newReleaseHeaderLabel,
QLabel#classicsHeaderLabel, QLabel#fantasyHeaderLabel, QLabel#nonFictionHeaderLabel {
   font-size: 14pt; /* Менший розмір */
   font-weight: 600;
   color: #343a40; /* Темно-сірий */
   margin-top: 15px;
   margin-bottom: 8px;
   padding-bottom: 4px;
   border-bottom: 1px solid #dee2e6; /* Світліша лінія */
}
QLabel#dateFilterLabel {
    color: #495057; /* Сірий текст */
    font-size: 9pt; /* Менший шрифт */
}

/* --- Кнопки категорій --- */
QWidget#categoriesWidget QPushButton {
    background-color: #ffffff;
    color: #0d6efd; /* Синій текст */
    border: 1px solid #ced4da; /* Сіра рамка */
    font-weight: 500;
    text-align: center;
    padding: 12px;
    min-height: 100px; /* Трохи менша висота */
    border-radius: 4px;
    transition: background-color 0.15s ease-in-out, border-color 0.15s ease-in-out, color 0.15s ease-in-out;
}
QWidget#categoriesWidget QPushButton:hover {
    background-color: #f8f9fa;
    color: #0b5ed7;
    border-color: #adb5bd;
}
QWidget#categoriesWidget QPushButton:pressed {
    background-color: #e9ecef;
}

/* --- Глобальний пошук --- */
QLineEdit#globalSearchLineEdit {
    min-height: 36px; /* Стандартна висота */
}
