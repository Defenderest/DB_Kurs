}

QLineEdit#globalSearchLineEdit {
    min-height: 30px; /* Ще зменшено висоту */
    background-color: #f1f3f5; /* Світліший фон */
    border: 1px solid #e9ecef; /* Дуже світла рамка */
    border-radius: 15px; /* Округлі краї (половина висоти) */
    padding-left: 15px; /* Відступ для тексту (і можливої іконки) */
    padding-right: 15px;
}
QLineEdit#globalSearchLineEdit:focus {
    border-color: #ced4da; /* Трохи темніша рамка при фокусі */
    background-color: #ffffff; /* Білий фон при фокусі */
}

/* Стиль для пошуку на сторінці книг */
QLineEdit#bookSearchLineEdit {
    min-height: 34px; /* Зменшено висоту */
    background-color: #f8f9fa; /* Світлий фон */
    border: 1px solid #dee2e6; /* Світла рамка */
    border-radius: 4px; /* Стандартне заокруглення */
    padding-left: 10px;
    padding-right: 10px;
}
QLineEdit#bookSearchLineEdit:focus {
    border-color: #86b7fe; /* Синя рамка при фокусі (як у інших полів) */
    background-color: #ffffff;
}


/* === Стилі для сторінок контенту === */
QWidget#discoverPage,
QWidget#booksPage,
QWidget#authorsPage,
QWidget#ordersPage,
QWidget#pageProfile {
    background-color: #ffffff; /* Білий фон для сторінок */
}

/* Стилі для вмісту сторінок (наприклад, ScrollArea) */
