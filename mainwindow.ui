QWidget {
    font-family: &quot;Segoe UI&quot;, system-ui, -apple-system, BlinkMacSystemFont, sans-serif;
    font-size: 10pt;
}

QTabWidget::pane {
    border: 1px solid #dee2e6;
    border-top: none;
    background-color: #ffffff;
    border-radius: 0 0 4px 4px;
    padding: 15px;
}

QTabBar::tab {
    padding: 10px 18px;
    margin-right: 2px;
    background-color: #e9ecef;
    border: 1px solid #dee2e6;
    border-bottom: none;
    color: #495057;
    font-weight: 500;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QTabBar::tab:selected {
    background-color: #ffffff;
    color: #495057; /* Змінено на темно-сірий */
    font-weight: 600;
    border-color: #dee2e6;
    border-bottom: 1px solid #ffffff;
    margin-bottom: -1px;
}

QTabBar::tab:hover:!selected {
    background-color: #dee2e6;
    color: #212529;
    border-color: #ced4da;
}

QTabWidget::tab-bar {
    alignment: left;
    left: 5px;
}

QLineEdit, QComboBox, QDateEdit, QSpinBox { /* Додано QSpinBox */
    padding: 8px 10px;
    border: 1px solid #ced4da;
    border-radius: 4px;
    background-color: #ffffff;
    color: #212529;
    min-height: 34px;
    font-size: 10pt;
}
QLineEdit:focus, QComboBox:focus, QDateEdit:focus {
    border-color: #adb5bd; /* Змінено на сірий */
    box-shadow: 0 0 0 0.2rem rgba(173, 181, 189, 0.25); /* Змінено колір тіні на сірий */
}

QLineEdit::placeholder {
    color: #6c757d;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid #ced4da;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}

QComboBox::down-arrow {
     image: url(D:/projects/DB_Kurs/QtAPP/untitled/icons//down-arrow.png);
     width: 10px;
     height: 10px;
}
QComboBox QAbstractItemView {
    border: 1px solid #ced4da;
    background-color: #ffffff;
    selection-background-color: #e9ecef; /* Змінено на світло-сірий */
    selection-color: #000;
    padding: 4px;
}

QPushButton {
    padding: 8px 14px;
    background-color: #6c757d; /* Змінено на сірий */
    color: white;
    border: 1px solid #6c757d; /* Змінено на сірий */
    border-radius: 8px; /* Збільшено заокруглення */
    font-weight: 500;
    min-height: 34px;
    font-size: 10pt;
    /* transition: background-color 0.15s ease-in-out, border-color 0.15s ease-in-out; */ /* transition is not a standard Qt property */
}
QPushButton:hover {
    background-color: #5a6268; /* Змінено на темніший сірий */
    border-color: #545b62; /* Змінено на темніший сірий */
}
QPushButton:pressed {
    background-color: #545b62; /* Змінено на темніший сірий */
    border-color: #4e555b; /* Змінено на ще темніший сірий */
}
QPushButton:disabled {
    background-color: #6c757d;
    color: #ffffff;
    border-color: #6c757d;
    opacity: 0.65;
}

QPushButton#cartButton, QPushButton#wishlistButton, QPushButton#profileButton {
    background-color: transparent;
    border: 1px solid transparent;
    padding: 6px;
    border-radius: 8px; /* Збільшено заокруглення */
    min-height: 36px; min-width: 36px;
    max-width: 36px; max-height: 36px;
    color: #495057;
    font-size: 14pt;
}
QPushButton#cartButton:hover, QPushButton#wishlistButton:hover, QPushButton#profileButton:hover {
    background-color: #e9ecef;
    color: #212529;
    border-color: #dee2e6;
}
QPushButton#cartButton:pressed, QPushButton#wishlistButton:pressed, QPushButton#profileButton:pressed {
    background-color: #dee2e6;
    border-color: #ced4da;
}

QPushButton#filterButton, QPushButton#sortButton, QPushButton#viewModeButton {
    background-color: #ffffff;
    border: 1px solid #ced4da;
    color: #495057;
    min-width: 36px; max-width: 36px;
    min-height: 34px; max-height: 34px;
    padding: 6px;
    border-radius: 8px; /* Збільшено заокруглення */
    font-size: 12pt;
}
QPushButton#filterButton:hover, QPushButton#sortButton:hover, QPushButton#viewModeButton:hover {
    background-color: #f8f9fa;
    border-color: #adb5bd;
    color: #212529;
}
QPushButton#filterButton:pressed, QPushButton#sortButton:pressed, QPushButton#viewModeButton:pressed {
    background-color: #e9ecef;
}
QPushButton#viewModeButton:checked {
    background-color: #e9ecef; /* Змінено на світло-сірий */
    border-color: #ced4da; /* Змінено на сірий */
    color: #495057; /* Змінено на темно-сірий */
}

QScrollArea {
    border: none;
    background-color: transparent;
}
QScrollBar:vertical {
    border: none;
    background: #f1f3f5;
    width: 6px;
    margin: 0px 0px 0px 0px;
    border-radius: 3px;
}
QScrollBar::handle:vertical {
    background: #adb5bd;
    min-height: 20px;
    border-radius: 3px;
}
QScrollBar::handle:vertical:hover {
    background: #6c757d;
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none; background: none; height: 0px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QScrollBar:horizontal {
    border: none; background: #f1f3f5; height: 6px; margin: 0px 0px 0px 0px; border-radius: 3px;
}
QScrollBar::handle:horizontal {
    background: #adb5bd; min-width: 20px; border-radius: 3px;
}
QScrollBar::handle:horizontal:hover {
    background: #6c757d;
}
QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    border: none; background: none; width: 0px;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}

QStatusBar {
    background-color: #e9ecef;
    color: #495057;
    font-size: 9pt;
    border-top: 1px solid #dee2e6;
}

QLabel {
   color: #212529;
   background-color: transparent;
}
QLabel#logoLabel {
   font-size: 16pt;
   font-weight: 600;
   color: #000000;
   padding-left: 0px;
}
/* Стиль для QLabel#bannerLabel більше не потрібен, оскільки банери встановлюються в коді */
/* QLabel#bannerLabel { ... } */

/* Стиль для міток банерів всередині QStackedWidget */
QLabel#bannerImageLabel { /* Використовуємо objectName, встановлений в UI */
   border-radius: 8px; /* Закруглені кути */
   background-color: #e0e0e0; /* Фоновий колір, якщо зображення не завантажено */
   /* background-image встановлюється в коді */
}


/* Стиль для самого QStackedWidget банера */
QStackedWidget#bannerStackedWidget {
    border-radius: 8px; /* Закруглені кути для контейнера */
    /* Не встановлюємо тут border, щоб не було подвійної рамки */
    /* Переконайтесь, що фон прозорий, щоб бачити мітки всередині */
    background-color: transparent;
}


QLabel#categoryHeaderLabel, QLabel#bestsellerHeaderLabel, QLabel#newReleaseHeaderLabel,
QLabel#classicsHeaderLabel, QLabel#fantasyHeaderLabel, QLabel#nonFictionHeaderLabel {
   font-size: 14pt; /* Трохи збільшено для акценту */
   font-weight: 600; /* Зроблено жирнішим */
   color: #212529; /* Темніший колір */
   margin-top: 25px; /* Збільшено верхній відступ */
   margin-bottom: 15px; /* Збільшено нижній відступ */
   padding-bottom: 3px;
   border-bottom: none; /* Лінія прибрана */
}
QLabel#dateFilterLabel {
    color: #495057;
    font-size: 9pt;
}

/* Стиль для кнопок категорій - більш легкий */
QWidget#categoriesWidget QPushButton {
    background-color: #f8f9fa; /* Світло-сірий фон */
    color: #343a40; /* Темніший текст */
    border: 1px solid #dee2e6; /* Світла рамка */
    font-weight: 500;
    text-align: center;
    padding: 8px; /* Ще зменшено padding */
    min-height: 70px; /* Ще зменшено висоту */
    border-radius: 8px; /* Збільшено заокруглення */
    /* transition: background-color 0.15s ease-in-out, border-color 0.15s ease-in-out, color 0.15s ease-in-out; */
}
QWidget#categoriesWidget QPushButton:hover {
    background-color: #e9ecef; /* Трохи темніший фон при наведенні */
    color: #212529;
    border-color: #ced4da; /* Трохи темніша рамка при наведенні */
}
QWidget#categoriesWidget QPushButton:pressed {
    background-color: #dee2e6; /* Ще темніший сірий при натисканні */
    border-color: #adb5bd;
}

/* Новий стиль для кнопок категорій в горизонтальному скролі */
QScrollArea#categoriesScrollArea QPushButton {
    background-color: #e9ecef; /* Світліший сірий фон */
    color: #343a40;
    border: 1px solid #dee2e6; /* Світла рамка */
    font-weight: 500;
    text-align: center;
    padding: 8px 18px; /* Горизонтальний padding більший */
    min-height: 38px; /* Зменшена фіксована висота */
    max-height: 38px;
    min-width: 100px; /* Мінімальна ширина */
    border-radius: 19px; /* Округлі краї (половина висоти) */
    margin-right: 10px; /* Відступ між кнопками */
}
QScrollArea#categoriesScrollArea QPushButton:hover {
    background-color: #dee2e6;
    color: #212529;
    border-color: #ced4da;
}
QScrollArea#categoriesScrollArea QPushButton:pressed {
    background-color: #ced4da;
    border-color: #adb5bd;
}
/* Спеціальний стиль для кнопки з іконкою (якщо потрібно) */
QScrollArea#categoriesScrollArea QPushButton#fictionCategoryButton {
    padding-left: 15px; /* Трохи менший лівий padding для іконки */
    /* Qt автоматично розмістить іконку ліворуч від тексту */
}

QLineEdit#globalSearchLineEdit {
    min-height: 30px; /* Ще зменшено висоту */
    background-color: #f1f3f5; /* Світліший фон */
    border: 1px solid #e9ecef; /* Дуже світла рамка */
    border-radius: 15px; /* Округлі краї (половина висоти) */
    padding-left: 15px; /* Відступ для тексту (і можливої іконки) */
    padding-right: 15px;
}
QLineEdit#globalSearchLineEdit:focus {
    border-color: #ced4da; /* Трохи темніша рамка при фокусі */
    background-color: #ffffff; /* Білий фон при фокусі */
}


/* === Стилі для сторінок контенту === */
QWidget#discoverPage,
QWidget#booksPage,
QWidget#authorsPage,
QWidget#ordersPage,
QWidget#pageProfile,
QWidget#bookDetailsPage /* Додано сторінку деталей книги */ {
