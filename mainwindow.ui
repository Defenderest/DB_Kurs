/* === Стилі для сторінки &quot;Мої замовлення&quot; === */
QLabel#ordersHeaderLabel {
    font-size: 18pt;
    font-weight: 600;
    color: #212529;
    margin-bottom: 10px; /* Відступ знизу */
}

QWidget#orderCardWidget {
    background-color: #ffffff;
    border: 1px solid #dee2e6;
    border-radius: 8px;
    padding: 18px 20px; /* Збільшено padding */
    margin-bottom: 15px; /* Відступ між картками */
}
QWidget#orderCardWidget:hover {
    border-color: #adb5bd;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.06); /* Легка тінь при наведенні */
}

QLabel#orderIdLabel {
    font-size: 11pt;
    font-weight: 600;
    color: #343a40;
}

QLabel#orderDateLabel, QLabel#orderTotalLabel {
    font-size: 10pt;
    color: #495057;
}

QLabel#orderStatusLabel {
    font-size: 10pt;
    font-weight: 500;
    padding: 3px 8px;
    border-radius: 12px; /* Округлі кути для статусу */
    color: #ffffff; /* Білий текст за замовчуванням */
    background-color: #6c757d; /* Сірий фон за замовчуванням */
    min-width: 80px; /* Мінімальна ширина для вирівнювання */
    qproperty-alignment: AlignCenter;
}
/* Стилі для різних статусів (можна додати більше) */
QLabel#orderStatusLabel[status=&quot;new&quot;] { background-color: #0d6efd; } /* Синій */
QLabel#orderStatusLabel[status=&quot;processing&quot;] { background-color: #ffc107; color: #343a40; } /* Жовтий */
QLabel#orderStatusLabel[status=&quot;shipped&quot;] { background-color: #0dcaf0; color: #343a40; } /* Бірюзовий */
QLabel#orderStatusLabel[status=&quot;delivered&quot;] { background-color: #198754; } /* Зелений */
QLabel#orderStatusLabel[status=&quot;cancelled&quot;] { background-color: #dc3545; } /* Червоний */

QPushButton#viewOrderDetailsButton {
    /* Використовує загальний стиль QPushButton */
    padding: 6px 12px; /* Менший padding */
    font-size: 9pt; /* Менший шрифт */
    min-height: 30px; /* Менша висота */
    background-color: #e9ecef; /* Світло-сірий фон */
    color: #495057; /* Темно-сірий текст */
    border-color: #dee2e6;
}
QPushButton#viewOrderDetailsButton:hover {
    background-color: #dee2e6;
    border-color: #ced4da;
    color: #212529;
}

QLabel#emptyOrdersLabel {
    font-size: 14pt;
    color: #6c757d;
    padding: 40px;
    background-color: #f8f9fa;
    border: 1px dashed #ced4da;
    border-radius: 8px;
    qproperty-alignment: AlignCenter;
}

/* Явно встановлюємо білий фон для області прокрутки деталей книги та її вмісту */
QScrollArea#bookDetailsScrollArea, QWidget#bookDetailsScrollContents {
    background-color: #ffffff;
    border: none; /* Прибираємо можливу рамку */
}

/* Об'єднаний стиль для всіх горизонтальних ScrollArea на головній сторінці */
QScrollArea#classicsScrollArea,
QScrollArea#fantasyScrollArea,
QScrollArea#nonFictionScrollArea {
    /* background-color, border-radius, padding видалено, щоб успадкувати від батька */
    border: none; /* Прибираємо рамку */
}

/* Стилі для індикаторів банера */
QWidget#bannerIndicatorWidget { /* Контейнер для індикаторів */
    background-color: transparent; /* Прозорий фон */
    margin-top: -15px; /* Піднімаємо індикатори трохи вище, накладаючи на банер */
    padding-bottom: 5px; /* Невеликий відступ знизу */
}

QRadioButton::indicator {
    width: 8px; /* Зменшено розмір */
    height: 8px;
    border-radius: 4px; /* Робимо його круглим */
    background-color: rgba(255, 255, 255, 0.5); /* Напівпрозорий білий для неактивного */
    border: 1px solid rgba(0, 0, 0, 0.2); /* Ледь помітна рамка */
    margin: 0 4px; /* Трохи збільшено відступи */
}

QRadioButton::indicator:checked {
    background-color: #ffffff; /* Колір активного індикатора (білий) */
    border: 1px solid rgba(0, 0, 0, 0.4); /* Чіткіша рамка для активного */
}

/* Прибираємо текст радіокнопки */
QRadioButton {
    spacing: 0;
    background: transparent;
}

